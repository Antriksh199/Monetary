<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Features"
                   namespace="Features.DL.Entities">

	<class name="UserPaymentMethod" table="Admin_User_Payment_Methods" lazy ="true" >

		<id name="Id" column="Id">
			<generator class="sequence" >
				<param name="sequence">AdminUserPaymentMethods_Id_Seq</param>
			</generator>
		</id>
		<many-to-one name="User" class="User" column="USER_ID" not-null="true" lazy ="proxy" fetch="join" />
		<many-to-one name="PaymentMethod" class="PaymentMethod" column="PAYMENT_METHOD_ID" not-null="true" lazy ="proxy" fetch="join"/>
		<property name="CreatedDate" column="created_date" type="DateTime"/>
		<property name="ModifiedDate" column="modified_date" type="DateTime" />
		<property name="CreatedBy" column="created_by" type="String" />
		<property name="ModifiedBy" column="modified_by" type="String" />

	</class>
</hibernate-mapping>
