<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Features"
                   namespace="Features.DL.Entities">

	<class name="Income" table="Income" >

		<id name="Id" column="Id">
			<generator class="sequence" >
				<param name="sequence">Income_Id_Seq</param>
			</generator>
		</id>

		<many-to-one name="Frequency" class="Frequency" column="FREQUENCY_ID"
					 lazy ="proxy"
					 fetch="join" />
		<many-to-one name="Source" class="IncomeType" column="INCOME_TYPE_ID"
					 lazy ="proxy"
					 fetch="join" />
		<many-to-one name="User" class="User" column="USER_ID"
					 lazy ="proxy"
					 fetch="join" />
		<many-to-one name="ReceivedIn"
					 column="U_PAYMENT_METHOD_ID"
					 class="UserPaymentMethod"
					 lazy ="proxy"
					 fetch="join"/>

		<property name="Value" column="AMOUNT" type="Decimal"/>
		<property name="ReceivedFrom" column="Received_From" type="String"/>
		<property name="Received" column="Received" type="Boolean"/>
		<property name="ReceivedOn" column="Received_On" type="DateTime"/>
		<property name="Description" column="Description" type="String"/>
		<property name="CreatedDate" column="created_date" type="DateTime"/>
		<property name="ModifiedDate" column="modified_date" type="DateTime" />
		<property name="CreatedBy" column="created_by" type="String" />
		<property name="ModifiedBy" column="modified_by" type="String" />


	</class>
</hibernate-mapping>