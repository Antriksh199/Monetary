<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Features"
                   namespace="Features.DL.Entities">

	<class name="ExpenseType" table="Admin_Expense_Type" lazy="true">

		<id name="Id" column="Id">
			<generator class="sequence" >
				<param name="sequence">AdminExpenseTypes_Id_Seq</param>
			</generator>
		</id>
		<property name="ExpenseCategoryId" column="Expense_Category_Id" type="int"/>

		<property name="Name" column="name" type="String"/>
		<property name="CreatedDate" column="created_date" type="DateTime"/>
		<property name="ModifiedDate" column="modified_date" type="DateTime" />
		<property name="CreatedBy" column="created_by" type="String" />
		<property name="ModifiedBy" column="modified_by" type="String" />

	</class>
</hibernate-mapping>